<!DOCTYPE html>
<html>
<head>
  <title>Chariteer</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>

<body>
  <nav>
    <ul>
      <!-- FOR USERS NOT ORGANIZATIONS -->
      <!-- If current user..... -->
      <li><%= link_to "Home", users_path%></li>
      <li><%= link_to "Profile", users_path(current_user.id)%></li>
      <li><%= link_to "Logout", session_path(current_user.id), method: :delete%></li>

    </ul>
  </nav>

<%= yield %>

</body>
</html>


<%= form_for @organization do |f| %>
<div><br>
    <div>
        <%= f.label :name %>
        <%= f.text_field :name %>
    </div><br>
    <div>
        <%= f.label :contact_email %>
        <%= f.email_field :contact_email %>
    </div><br>
    <div>
        <%= f.label :mission_statement %>
        <%= f.text_field :mission_statement %>
    </div><br>
    <div>
        <%= f.label :website %>
        <%= f.text_field :website %>
    </div><br>
    <div>
        <%= f.label :non_profit %>
        <%= f.text_field :non_profit %>
    </div><br>
    <div>
        <%= f.label :password %>
        <%= f.password_field :password %>
    </div><br>
        <%= f.submit "Sign Up" %>
</div>

<% end %>

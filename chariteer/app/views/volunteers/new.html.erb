<h1>Volunteers#new</h1>
<%= form_for @volunteer do |f| %>
<div><br>
    <div>
        <%= f.label :first_name %>
        <%= f.text_field :first_name %>
    </div><br>
     <div>
        <%= f.label :last_name %>
        <%= f.text_field :last_name %>
    </div><br>

    <div>
        <%= f.label :email %>
        <%= f.email_field :email %>
    </div><br>

    <div>
        <%= f.label :password %>
        <%= f.password_field :password %>
    </div><br>
        <%= f.submit "Sign Up" %>
</div>

<% end %>
